;Window Close
^q::Send,% WinActive("ahk_class IEFrame") ? "!{F4}" : "!{Space}c"

;yyyyMMdd
^@::
	FormatTime, TimeString, ,yyyyMMdd
	Send, %TimeString%
	return

;yyyy/MM/dd
^sc027::
	FormatTime, TimeString, ,yyyy/MM/dd
	Send, %TimeString%
	return

;HHmmss
^[::
	FormatTime, TimeString, ,HHmmss
	Send, %TimeString%
	return

;HHmmss
^sc028::
	FormatTime, TimeString, ,HH:mm:ss
	Send, %TimeString%
	return

